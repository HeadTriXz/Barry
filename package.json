{
  "name": "barry",
  "version": "1.1.0",
  "description": "Barry is a versatile Discord bot that simplifies server management with a range of features such as moderation, leveling, and marketplace tools.",
  "repository": "HeadTriXz/Barry",
  "author": "HeadTriXz",
  "license": "MIT",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "db:generate": "dotenv -- turbo run db:generate",
    "db:push": "dotenv -- turbo run db:push",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "start": "dotenv -- turbo run start --env-mode=loose",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@eslint/js": "^8.39.0",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-v8": "^0.34.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.39.0",
    "rimraf": "^5.0.0",
    "tsup": "^7.1.0",
    "turbo": "^2.0.7",
    "typescript": "^5.2.2",
    "vitest": "^0.34.1",
    "vitest-mock-extended": "^1.2.0"
  }
}
